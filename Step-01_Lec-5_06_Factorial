#include<bits/stdc++.h>
using namespace std;

int factorial(int X)
{
    int n=1;
    for(int i=1; i<=X; i++){
        n= n*i;
    }
    return n;
}
int main(){
    int X;
    cout<<"enter number to find factorial : ";
    cin>>X;
    int result= factorial(X);
    cout<<"the factorial is: "<<result;
}
