#include<bits/stdc++.h>
using namespace std;

void func(int i, int n){
   
   // Base Condition.
   if(i>n) return;
   cout<<"Arya"<<endl;

   // Function call to print till i increments.
   func(i+1,n);

}

int main(){
  
  // Here, letâ€™s take the value of n 
  int n;
  cout<<"enter n : ";
  cin>>n;
  func(1,n);
  return 0;

}
